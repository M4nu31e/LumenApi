{
  "name": "ix/fraud-protection-service",
  "description": "IX Fraud Protection Statistics",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "elasticsearch/elasticsearch": "~6.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/redis": "^5.2",
    "ix-intern/laravel-helper": "^4.0",
    "laravel/lumen-framework": "5.8.*",
    "league/flysystem-aws-s3-v3": "~1.0",
    "predis/predis": "^1.1",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "laracademy/generators": "^1.5",
    "laravel/tinker": "^1.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "repo-name": {
      "type": "git",
      "url": "git@gitlab.muc.internetx.com:ix-intern/laravel-helper.git"
    }
  }
}
